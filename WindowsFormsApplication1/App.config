<?xml version="1.0" encoding="utf-8" ?>
<configuration>
    <startup> 
        <supportedRuntime version="v4.0" sku=".NETFramework,Version=v4.5" />
    </startup>
  <appSettings>
    
    <!-- 
    The value below represents the folder where the application will check for updates 
    in the form of a new release - which you create using squirrel.
    Examples below
    -->
    <!--<add key="UpdatePathFolder" value="http://applications/StxXmlService/Development/"/>-->
    <!--<add key="UpdatePathFolder" value="\\serverpath\foldername"/>-->
    <add key="UpdatePathFolder" value="c:\updatesForMyApp\"/>
    
    <!-- 
    Dont have any spaces or dots (.) an your nuget packageId inside your nupkg file
    e.g windows.service wont work however windowsservice will work :)
    -->
    <add key="PackageID" value="WindowsFormsApplication1"/>
  </appSettings>
</configuration>